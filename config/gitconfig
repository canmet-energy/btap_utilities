  os-spreadsheet = https://github.com/NREL/OpenStudio-analysis-spreadsheet.git
  nrcan-ptool = clone https://github.com/phylroy/OpenStudio-PTool.git
  nrcan-standards = clone https://github.com/NREL/openstudio-standards.git -b nrcan 
  nrcan-vis-pc = clone https://github.com/phylroy/parallel-coordinates.git
  nrcan-vis-sankey = clone https://github.com/phylroy/d3-sankey.git
  co = checkout
  ci = commit
  st = status
  br = branch
  conflicts = diff --name-only --diff-filter=U
  hist = log --pretty=format:\"%C(yellow)%h %ad | %C(reset)%sd %C(green)[%an]\" --date=short --graph
  type = cat-file -t
  dump = cat-file -p
